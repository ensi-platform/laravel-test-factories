{
    "name": "ensi/laravel-test-factories",
    "description": "",
    "keywords": [
        "laravel",
        "factory"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Natalia Golubkova",
            "email": "golubkova@greensight.ru"
        }
    ],
    "require": {
        "php": "^8.0",
        "ensi/test-factories": ">=0.2",
        "guzzlehttp/guzzle": "^6.0 || ^7.0",
        "laravel/framework": "^8.0 || ^9.0 || ^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2",
        "php-parallel-lint/php-var-dump-check": "^0.5.0"
    },
    "autoload": {
        "psr-4": {
            "Ensi\\LaravelTestFactories\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ensi\\LaravelTestFactories\\LaravelTestServiceProvider"
            ]
        }
    },
    "scripts": {
        "cs": "php-cs-fixer fix --config .php-cs-fixer.php"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
